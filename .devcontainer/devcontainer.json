{
	"name": "localisation-services",
	"dockerComposeFile": "../docker-compose.yml",
	"service": "dev",
	"workspaceFolder": "/workspace",	
	"containerEnv": {
		"SHELL": "/bin/zsh"
	},
	"features": {
		"terraform": {
			"version": "1.6.1",
			"tflint": "latest",
			"terragrunt": "0.52.1"
		},
		"aws-cli": {
			"version": "latest"
		}
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"hashicorp.terraform",
				"github.copilot",
				"github.vscode-github-actions"
			]
		}
	},
	"remoteUser": "vscode"
}